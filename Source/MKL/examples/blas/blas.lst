BLAS = $(L1) $(L2) $(L3)
#--------------------------------------------------------------------------
L1 = $(S1) $(D1) $(C1) $(Z1)
L2 = $(S2) $(D2) $(C2) $(Z2)
L3 = $(S3) $(D3) $(C3) $(Z3)
#--------------------------------------------------------------------------
S1 =  sasum+ saxpyi+ saxpy+ scopy+  sswap+ isamax+ isamin+  snrm2+  sdot+ \
      sdoti+   srot+ srotg+ srotm+ srotmg+  sscal+  sroti+
